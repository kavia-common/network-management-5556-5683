# Frontend .env example (Create React App)
# Copy this file to `.env` and adjust values as needed.

# Base URL for the Backend API (Flask). For local dev when backend runs on port 3001:
REACT_APP_API_BASE_URL=http://localhost:3001

# Use in-memory mock API instead of calling backend (true/false)
REACT_APP_USE_MOCKS=false

# Default page size for pagination on the devices list
REACT_APP_PAGINATION_PAGE_SIZE_DEFAULT=10
