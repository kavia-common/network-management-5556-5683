# Optimize CRA dev server performance
# Disables generating heavy source maps which can slow builds/rebuilds
GENERATE_SOURCEMAP=false
# Ensure React Fast Refresh remains enabled for quick HMR
FAST_REFRESH=true
# Prevent dev server from auto-opening browser in CI/containers
BROWSER=none
# Ensure CRA does not run in CI mode during local dev (affects watch behavior)
CI=false

# Default port for CRA dev server
PORT=3000
